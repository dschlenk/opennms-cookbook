 GPRINT:sent:MIN:"Min  \\: %7.2lf %s" \
 GPRINT:sent:MAX:"Max  \\: %7.2lf %s\\n" \
 LINE2:rec#00ff00:"BytesRec " \
 GPRINT:rec:AVERAGE:" Avg  \\: %7.2lf %s" \
 GPRINT:rec:MIN:"Min  \\: %7.2lf %s" \
 GPRINT:rec:MAX:"Max  \\: %7.2lf %s\\n"

