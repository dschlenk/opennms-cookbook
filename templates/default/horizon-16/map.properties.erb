<% if @cmenu[:telnet][:enabled] -%>
cmenu.Telnet.link=<%=@cmenu[:telnet][:link]%>
<% else -%>
#cmenu.Telnet.link=telnet://ELEMENT_HOSTNAME
#cmenu.Telnet.link=telnet://ELEMENT_IP
<% end -%>
cmenu.Ping.link=<%=@cmenu[:ping][:link]%>
